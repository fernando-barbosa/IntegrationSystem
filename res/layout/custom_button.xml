<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#1E641E"
                android:endColor="#145014"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#cccccc" />
            <corners 
                android:bottomRightRadius="30dp" 
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp" 
                android:topRightRadius="30dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>

    <item>        
        <shape>
            <gradient
                android:endColor="#1E781E"
                android:startColor="#3CB43C"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#aaaaaa" />
            <corners 
                android:bottomRightRadius="30dp" 
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp" 
                android:topRightRadius="30dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>